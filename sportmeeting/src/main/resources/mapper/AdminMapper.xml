<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tyut.sport.mapper.AdminMapper">

    <select id="QueryAdmin" resultType="admin">
        select * from admin where name=#{username} and password=#{password}
    </select>


    <select id="QueryAdminInfor" resultType="admin">
        select * from admin where name=#{username}
    </select>

</mapper>
